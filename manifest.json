{

  "manifest_version": 2,
  "name": "Bookmark Page",
  "version": "1.0",

  "description": "Adds user's bookmarks to an html page",
  "homepage_url": "https://github.com/hanycopes/firefox-extension",

  "chrome_url_overrides": {
    "newtab": "start_page/start_page.html"
  },

  "permissions": [
    "bookmarks"
  ]

}